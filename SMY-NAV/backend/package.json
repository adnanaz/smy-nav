{
  "name": "smy-nav-backend",
  "version": "1.0.0",
  "description": "SMY-NAV Maritime Training Management System - Backend API",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'Build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "yarn prisma generate",
    "db:push": "yarn prisma db push",
    "db:migrate": "yarn prisma migrate dev",
    "db:reset": "yarn prisma migrate reset",
    "db:seed": "node prisma/seed.js",
    "db:studio": "yarn prisma studio",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "maritime",
    "training",
    "management",
    "api",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "SMY-NAV Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@prisma/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.7",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/smy-nav-backend.git"
  }
}
